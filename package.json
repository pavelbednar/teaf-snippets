{
  "name": "teaf-snippets",
  "displayName": "teaf-snippets",
  "description": "This extension contains snippets for TEAF development.",
  "repository": {"url": "https://github.com/pavelbednar/teaf-snippets"},
  "author": {"name": "Pavel Bednář"},
  "publisher": "PavelBednar",
  "license": "LGPL-3.0-only",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/snippets.code-snippets"
